<script>

    new Glide(".images", {
        type: 'carousel',
        perView: 3,
        focusAt: 'center',
        gap: 20,
        rewind: true,
        swipeThreshold: 50,
        autoplay: 5000,
        dragThreshold: 120,
        startAt:0,
        perTouch: 1,
        rewind: true,
        animationDuration: 400,
        breakpoints:
        {1920:{
            perView: 4

        },
        1366: {
            perView: 3
            
        },
            800: {
                perView: 1.4
            },
          
            }
    }).mount()
    setInterval(function(){
        var array1=document.getElementsByClassName('glide__slide')
        var width=window.innerWidth
        function setHeadingUp(){
            var heading=document.getElementsByClassName('heading')
                heading[0].innerHTML=`<h1 class="heading-font fntsz-2 mt-4 text-uppercase text-center mx-auto">Upcoming event<h1>`
        }
        function setHeadingPrev(){
            
            var heading=document.getElementsByClassName('heading')
            heading[0].innerHTML=`<h1 class="heading-font fntsz-2 mt-4 text-uppercase text-center mx-auto">previous event<h1>`
        }
        setHeadingPrev()
       for(var i=0;i<=array1.length;i++){
        if(width<=800){
            
            if(array1[i].classList[2]=='glide__slide--active' && array1[i].classList[1]=='m'){
                
                setHeadingUp()
                break  
               
            }
            
        }
        else{
            if(array1[i].classList[2]=='glide__slide--active' && array1[i].classList[1]=='m'){
                
                setHeadingUp()
                break 
            }
      
        }}},1000)
    
    
    </script>
